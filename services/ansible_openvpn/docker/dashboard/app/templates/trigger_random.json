{
  "index": "sensor_yamnet_*",
  "size": 1,
  "_source": true,
  "query": {
    "function_score": {
      "query": {
        "bool": {
          "must": [
            {
              "range": {
              "date": {
                "gte": "2024-06-01",
                "lte": "2024-11-31"
                }
              }
            }
          ],
          "must_not": [
            {"exists": {"field": "annotation"}}
          ]
        }
      },
      "random_score": {}
    }
  }
}